<h1>Listing contacts</h1>
<p id="notice"><%= notice %></p>
<table border ="1">
  <tr>
    <th><b> Name</b></th>
    <th>Address</th>
    <th>City</th>
    <th>Pin code</th>
    <th>State</th>
    <th>Country</th>
    <th>Phone(H)</th>
    <th>Phone(O)</th>
    <th>Email</th>
    <th>Manage</th>
    
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.name %></td>
    <td><%= contact.address %></td>
    <td><%= contact.city %></td>
    <td><%= contact.pin_code %></td>
    <td><%= contact.state %></td>
    <td><%= contact.country %></td>
    <td><%= contact.phone_number_home %></td>
    <td><%= contact.phone_number_office %></td>
    <td><%= contact.email %></td>
    <td><%= link_to 'Show', contact %> <%= link_to 'Edit', edit_contact_path(contact) %> <%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
